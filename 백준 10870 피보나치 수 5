import java.util.*;

public class Main{
    public static void main(String[] args){
        
        Scanner sc = new Scanner(System.in);
        
        int input = sc.nextInt();
           
        System.out.print(fibonacci(input));
    }
    
    public static int fibonacci(int n){
        
        if(n <=1){
            return n;
        }
        
        return fibonacci(n-1) + fibonacci(n-2);    
       
    }
    
}
