class Solution {
    public boolean solution(String s) {
        boolean answer = true;
        
        if(s.length() != 4 && s.length() != 6) return false;
        
         char temp;
         for(int i =0;i<s.length();i++){
             temp = s.charAt(i);
             if((temp >='A' && temp <= 'Z') || (temp >='a' && temp <= 'z')) return false;
         }

        return answer;
    }
}
