class Solution {
    public String solution(String phone_number) {
        String answer = "";
        
        int length = phone_number.length();
        
        String nam4 = phone_number.substring(length-4,length);
         
        for(int i=0;i<length-4;i++){
            answer = answer+"*";
        }
        
        answer= answer+nam4;
        return answer;
    }
}
